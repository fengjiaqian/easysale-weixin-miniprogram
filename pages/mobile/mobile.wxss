.tel-log {
    width: 130rpx;
    height: 130rpx;
    display: block;
    margin: 100rpx auto 60rpx auto;
}

.tel-name {
    width: 100%;
    text-align: center;
    font-weight: bold;
    font-size: 32rpx;
    color: #333;
}

.tel-btn {
    width: 702rpx;
    height: 94rpx;
    margin: 80rpx auto 0 auto;
    display: block;
    font-size: 32rpx;
    color: white;
    line-height: 94rpx;
    border-radius: 10rpx;
    background-color: rgba(255, 86, 56, 1);
}